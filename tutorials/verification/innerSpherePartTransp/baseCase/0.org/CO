FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      CO;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform yCOSet;

boundaryField
{
    infinity
    {
        type            fixedValue;
        value           uniform yCOSet;
    }
    sphere
    {
        type            fixedValue;
        value           uniform yCOSet;
    }
}

