FoamFile
{
    version     1.0;
    format      ascii;
    class       volScalarField;
    object      CO;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform yCOSet;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform yCOSet;
    }
    outlet
    {
        type            zeroGradient;
    }
    sides
    {
        type            symmetry;
    }
    // sphere
    // {
    //     type            fixedValue;
    //     value           uniform yCOSet;
    // }
    
