FoamFile
{
    version     1.0;
    format      ascii;
    class       volScalarField;
    object      CO;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform yCOSet;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform yCOSet;
    }
    outlet
    {
        type            zeroGradient;
    }
    sides
    {
        type            zeroGradient;
    }
    sphere
    {
        type            fixedValue;
        value           uniform yCOSet;
    }
    
}

