FoamFile
{
    version     1.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    inlet
    {
        type    totalPressure;
        p0      uniform 101325;
        value   uniform 101325;
    
        U       U;
        phi     phi;
    }
    outlet
    {
        type    zeroGradient;
    }
    sides
    {
        type    zeroGradient;
    }
    sphere
    {
        type    totalPressure;
        p0      uniform 101325;
        value   uniform 101325;
    
        U       U;
        phi     phi;
    }
}

